{
  "name": "restaurant-reservation-backend",
  "version": "1.0.0",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:dev": "nodemon src/server.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:reseed": "npm run db:rollback -- -all || true && npm run db:migrate && npm run db:seed",
    "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "test": "jest --runInBand",
    "lint": "eslint .",
    "format": "prettier -w ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "eslint": "^9.11.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  }
}
